  <script>
    function updateVisitorCounter() {
        // Gera um número aleatório entre 300 e 500
        var min = 300;
        var max = 500;
        var randomNumber = Math.floor(Math.random() * (max - min + 1)) + min;

        // Atualiza o conteúdo do elemento .visitor-counter
        var visitorCounterElement = document.querySelector('.visitor-counter');
        if (visitorCounterElement) {
            visitorCounterElement.textContent = randomNumber + ' pessoas visualizaram o produto em 24 horas';
        } else {
            console.error('Elemento com a classe .visitor-counter não encontrado.');
        }
    }

    // Chama a função para atualizar o contador quando o documento estiver pronto
    document.addEventListener('DOMContentLoaded', function() {
        updateVisitorCounter();
    });
</script>

  
