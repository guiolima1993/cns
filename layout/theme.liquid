    <script>
  document.addEventListener('DOMContentLoaded', function() {
    var paramsToPropagate = [
      'utm_source',
      'utm_medium',
      'utm_campaign',
      'utm_content',
      'utm_term',
      'affiliate',
      'promocode'
    ];

    function getQueryParams() {
      var params = {};
      var query = window.location.search.substring(1);
      var vars = query.split('&');
      for (var i = 0; i < vars.length; i++) {
        var pair = vars[i].split('=');
        if (paramsToPropagate.indexOf(pair[0]) > -1) {
          params[pair[0]] = decodeURIComponent(pair[1]);
        }
      }
      return params;
    }

    function addQueryParamsToUrl(url, params) {
      var parser = new URL(url);
      for (var key in params) {
        parser.searchParams.set(key, params[key]);
      }
      return parser.href;
    }

    var params = getQueryParams();
    var anchors = document.querySelectorAll('a');
    anchors.forEach(function(anchor) {
      var anchorHost = new URL(anchor.href).hostname;
      if (anchorHost === 'seguro.cleannewstore.com') {
        anchor.href = addQueryParamsToUrl(anchor.href, params);
      }
    });
  });
</script>
    
